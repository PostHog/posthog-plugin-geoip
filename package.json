        "@typescript-eslint/eslint-plugin": "^4.12.0",
        "@typescript-eslint/parser": "^4.12.0",
        "eslint": "^7.0.0",
        "husky": "~4.3.6",
        "jest": "^26.6.3",
        "lint-staged": "~10.5.3",
        "prettier": "^2.2.1",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && tsc --noEmit"
